      "template_path": "prompt.jinja",
      "enhance_positive": "true",
      "reuse_running_model": "true",
      "fallback_model": "llama3.2:3b",
      "fallback_force_cpu": "true",
      "seed": 0,
      "clip": ["CheckpointLoaderSimple", 1]
