  "KSampler": {
    "inputs": {
      "seed": 295312109359859,
      "steps": 25,
      "cfg": 1,
      "sampler_name": "euler",
      "scheduler": "simple",
      "denoise": 1,
      "model": ["CheckpointLoaderSimple", 0],
      "positive": ["OllamaEnhancerNode", 0],
      "negative": ["OllamaEnhancerNode", 1],
      "latent_image": ["EmptyLatentImage", 0]
